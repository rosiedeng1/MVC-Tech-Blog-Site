<!-- Loop over each object in the posts array -->
{{!--html to show all the posts being looped --}}

{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>

    <p>{{post.title}}: {{post.content}}: {{post.date_created}} {{post.user.username}} </p>
  </div>
</div>
{{/each}}